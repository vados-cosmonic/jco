{
  "name": "@bytecodealliance/jco-std",
  "version": "0.0.1",
  "description": "Library of shared functionality for use with the JS WebAssembly components",
  "contributors": [
    {
      "name": "Victor Adossi",
      "email": "vadossi@cosmonic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytecodealliance/jco.git"
  },
  "license": "(Apache-2.0 WITH LLVM-exception)",
  "bugs": {
    "url": "https://github.com/bytecodealliance/jco/issues"
  },
  "homepage": "https://github.com/bytecodealliance/jco#readme",
  "keywords": [
    "Wasm",
    "WebAssembly",
    "Component"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "src/index.ts",
  "exports": {
    "./wasi/0.2.x/http/adapters/hono": {
      "types": "./dist/wasi/0.2.x/http/adapters/hono.d.ts",
      "browser": "./dist/wasi/0.2.x/http/adapters/hono.js",
      "default": "./dist/wasi/0.2.x/http/adapters/hono.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "lint": "eslint -c ./eslint.config.mjs",
    "lint:fix": "npm run lint -- --fix",
    "build:bindings:wasi:http:0.2.x": "WIT_PATH=wit/ WIT_WORLD=http-v0m2px WASI_HTTP_VERSION=0.2.4 OUTPUT_DIR_PATH=src/wasi/0.2.x/generated/types node scripts/generate-wasi-bindings.mjs",
    "build:ts": "tsc",
    "build": "npm run build:bindings:wasi:http:0.2.x && npm run build:ts",
    "test": "vitest run -c test/vitest.ts",
    "setup:vendor": "npm run -w @bytecodealliance/jco build:release && cp ../jco/obj/*.core*.wasm vendor && cp ../jco/obj/*.js vendor && cp ../jco/obj/*.ts vendor && cp -r ../jco/obj/interfaces vendor",
    "prepack": "npm run setup:vendor"
  },
  "devDependencies": {
    "@bytecodealliance/jco-transpile": "^0.0.3",
    "@bytecodealliance/preview2-shim": "^0.17.2",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/which": "^3.0.4",
    "hono": "^4.9.10",
    "rolldown": "^1.0.0-beta.42",
    "which": "^2.0.2"
  }
}
